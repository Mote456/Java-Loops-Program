//sum of digit square==That number 

package Loop;

public class NeonNumber 
{

	public static void main(String[] args)
	{
		for(int i=1;i<=1000;i++)
		{
			int s=i*i;
			int sum=0;
			while(s>0)
			{
				int d=s%10;
				    sum=sum+d;
				    s=s/10;
			}
			if(sum==i)
				System.out.print(" "+i);
		}

	}

}
