package Loop;

public class Question209 {

	public static void main(String[] args)
	{
		int f,c=0;
		System.out.println("Print first 10 prime number=");
		for(int i=1;i<=100;i++)
		{
			f=0;
			for(int j=1;j<=i;j++)
			{
				if(i%j==0)
					f++;
			}
			if(f==2)
			{
				c++;
				if(c<11)
				   System.out.println(" "+i);
			}
		}

	}

}
