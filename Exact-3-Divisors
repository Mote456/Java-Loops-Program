package Loop;

public class Exact3divisors {

	public static void main(String[] args)
	{
		int d;
		System.out.println("Print exact 3 divisiors Number=");
		for(int i=1;i<=1000;i++)
		{
			d=0;
			for(int j=1;j<=i;j++)
			{
				if(i%j==0)
					d++;
			}
			if(d==3)
				System.out.println(" "+i);
		}

	}

}
